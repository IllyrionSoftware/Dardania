{
  "json.schemas": [
    {
      "fileMatch": ["/.vscode/c_cpp_properties.json"],
      "url": "https://raw.githubusercontent.com/microsoft/vscode-cpptools/main/Extension/c_cpp_properties.schema.json"
    }
  ],
  "configurations": [
    {
      "name": "Linux",
      "includePath": ["${workspaceFolder}/**"],
      "defines": [],
      "compilerPath": "/usr/bin/gcc",
      "cStandard": "c23",
      "cppStandard": "c++23",
      "intelliSenseMode": "gcc-x64"
    }
  ],
  "version": 4
}
